(set-option :produce-models true)
(set-logic QF_NIA)
(declare-fun x () Int)
(declare-fun y () Int)
(declare-fun z () Int)
(declare-fun w () Int)
(assert (> x 0))
(assert (> y 0))
(assert (> z 0))
(assert (> w 0 ))
(assert (distinct x y))
(assert (distinct x z))
(assert (distinct y z))
(assert (= (+ (* x x) (* y y)) (* z z)))
(assert (= (+ z (+ x y)) 1000))
(assert (= (* x (* y z)) w))
(check-sat)
